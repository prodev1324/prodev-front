<% _.each(points, function(point, name) { %>
.change-entry
    .activity-changed
        span(translate="ACTIVITY.US_POINTS", translate-values!="<%- name.toLowerCase() %>")
    .activity-fromto
        p
            strong(translate="COMMON.FROM")
            br
            span <%- point[0] %>
        p
            strong(translate="COMMON.TO")
            br
            span <%- point[1] %>
<% }); %>