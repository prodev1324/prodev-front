<% _.each(tags, function(tag) { %>
span(class="tag", style!="border-left: 5px solid <%- tag.color %>;")
    span.tag-name <%- tag.name %>
    <% if (isEditable) { %>
    svg.icon.icon-close.remove-tag(title="{{ 'COMMON.TAGS.DELETE' | translate }}")
        use(xlink:href="#icon-close")
    <% } %>
<% }); %>
